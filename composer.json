{
  "name": "sebastianlenz/openinghoursfield",
  "description": "A field for Craft CMS that manages rule based opening hours",
  "type": "craft-plugin",
  "license": "MIT",
  "require": {
    "ext-json": "*",
    "craftcms/cms": "^4.0.2",
    "sebastianlenz/craft-utils": "^3.0.6"
  },
  "autoload": {
    "psr-4": {
      "lenz\\openinghoursfield\\": "src/"
    }
  },
  "extra": {
    "handle": "openinghoursfield",
    "name": "Opening Hours Field",
    "developer": "Sebastian Lenz",
    "developerUrl": "https://github.com/sebastian-lenz/"
  },
  "config": {
    "allow-plugins": {
      "yiisoft/yii2-composer": true,
      "craftcms/plugin-installer": true
    },
    "platform": {
      "php": "8.0.2"
    }
  }
}
